<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false">
        <shape android:shape="rectangle" android:useLevel="false">
            <!--<corners android:radius="10dp"></corners>-->
            <corners android:topLeftRadius="10dp" android:topRightRadius="10dp"></corners>
            <solid android:color="#E9EAEE" />
            <stroke android:width="1dp" android:color="#E9EAEE" />
        </shape>

    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle" android:useLevel="false">
            <corners android:topLeftRadius="10dp" android:topRightRadius="10dp"></corners>
            <solid android:color="#385696" />
            <stroke android:width="1dp" android:color="#385696" />
        </shape>
    </item>
</selector>