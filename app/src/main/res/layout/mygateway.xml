<?xml version="1.0" encoding="utf-8"?><!--用于选择网关界面-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/p96"
        android:text="@string/mygate"
        android:textColor="@color/e30"
        android:textSize="@dimen/p26" />

    <ImageView
        android:id="@+id/iconimage_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/p75"
        android:background="@drawable/bodyh" />

    <RelativeLayout
        android:id="@+id/add_id"
        android:layout_width="@dimen/p170"
        android:layout_height="@dimen/p170"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/p50"></RelativeLayout>

    <ListView
        android:id="@+id/mygatecir"
        android:layout_width="wrap_content"
        android:layout_height="340dp"
        android:divider="#00000000"
        android:dividerHeight="18dp"
        android:layout_alignTop="@+id/iconimage_id"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></ListView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/p129"
        android:text="@string/add"
        android:textColor="@color/e2c"
        android:textSize="@dimen/p20" />
    <include
        android:id="@+id/maintitledialog"
        layout="@layout/maintitledialog" />
</RelativeLayout>